{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "no-console": false,
        "prettier": true,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}